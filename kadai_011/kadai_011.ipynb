      "source": [
        "# 課題）繰り返し処理を使いこなそう\n",
        "\n",
        "##　for文を使用した場合\n",
        "array = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "for value in array:\n",
        "    print(value)\n",
        "\n",
        "##　while文を使用した場合\n",
        "array = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "i = 0\n",
        "while i < len(array):\n",
        "    value = array[i]\n",
        "    print(value)\n",
        "    i = i + 1"
      ]
    }
  ]
