        "# 課題15）クラスを使いこなそう\n",
        "\n",
        "class Human:\n",
        "    def __init__(self,name,age):\n",
        "        self.name = name\n",
        "        self.age = age\n",
        "    def printinfo(self):\n",
        "        print(f\"名前：{self.name}、年齢：{self.age}歳\")\n",
        "\n",
        "Taro = Human(\"侍太郎\",29)\n",
        "Taro.printinfo()"
      ]
